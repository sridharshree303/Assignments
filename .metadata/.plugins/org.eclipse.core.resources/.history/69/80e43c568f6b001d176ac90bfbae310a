package introToPrblmSolving;

public class CountFactors {
	
	public static void main(String[] args) {
		System.out.println(solve((long) 125));
	}
    public static int solve(Long A) {
    	int count = 0;
    	for(int i=1; i <= A ; i++) {
    		if(A % i == 0)
    			count++;
    	}
    	return count;
    }
}
