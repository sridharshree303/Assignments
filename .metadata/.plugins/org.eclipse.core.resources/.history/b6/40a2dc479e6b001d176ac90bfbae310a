package introToPrblmSolving;

public class FindFloor {
	public static void main(String[] args) {
		System.out.println(floor(-624));
		System.out.println(Math.sqrt(-4));
	}
	
	public static int floor(int value) {
		for(int i= 1; i <= value; i++) {
			if(i*i > value) {
				return i-1;
			}
		}
		return 0;
	}
}
