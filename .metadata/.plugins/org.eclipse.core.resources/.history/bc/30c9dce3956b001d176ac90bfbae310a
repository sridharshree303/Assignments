package introToPrblmSolving;

public class IsPrime {
	public static void main(String[] args) {
		System.out.println(solve((long) 101));
	}
	
	public static int solve(Long A) {
		int count = 0;
		for(int i = 1; i <= A; i++) {
			if(A % i == 0) {
				if(i == A/i) {
					count++;
				}else {
					count = count+2;System.out.println(count+" - "+ i);
				}
			}
		}
		if(count <= 2) {
			return 1;
		}
		return 0;
	}
}
