package introToPrblmSolving;

public class CountPrimes {
	public static void main(String[] args) {
		System.out.println(solve(19));
	}
	
	public static int solve(int A) {
		int count = 0;
		for(int i=1; i <= A; i++) {
			if(isPrime(i)) {
				System.out.print(i+" ");
				count++;
			}
		}
	
		return count;
    }
	
	public static boolean isPrime(int num) {
		if(num <= 1) {
			return false;
		}
		
		for(int i=2;i < num;i++) {
			if(num % i == 0) {
				return false;
			}
		}
		return true;
	}
}
